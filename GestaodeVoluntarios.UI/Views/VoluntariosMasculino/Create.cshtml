@model GestaodeVoluntarios.UI.Models.Masculino.MasculinoCadastro
<style>

    legend {
        background-color: #EFEFEF;
        color: black;
        padding: 5px 20px;
    }

    input {
        margin: 5px;
    }
</style>

<div class="col-sm-12">
    <form asp-action="Create" method="post" id="formMasculinoCadastro">
        <div class="page-header card">
            <div class="card-block">
                <h5 class="m-b-10">Voluntário Masculino</h5>
                <ul class="breadcrumb-title b-t-default p-t-10">
                    <li class="breadcrumb-item">
                        <a href="\Home\index"> <i class="fa fa-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="\VoluntariosMasculino\index">Voluntário Masculino</a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="alert alert-warning background-danger" role="alert" id="alertNome" style="display:none">
            <strong>Atenção!</strong> O campo Nome é obrigatório!
        </div>
        <div class="alert alert-warning background-danger" role="alert" id="alertClasseSocial" style="display:none">
            <strong>Atenção!</strong> O campo Classe Social é obrigatório!
        </div>
        <div class="alert alert-warning background-danger" role="alert" id="alertCPF" style="display:none">
            <strong>Atenção!</strong> O campo CPF é obrigatório!
        </div>
        <div class="alert alert-warning background-danger" role="alert" id="alertRG" style="display:none">
            <strong>Atenção!</strong> O campo RG é obrigatório!
        </div>
        <div class="alert alert-warning background-danger" role="alert" id="alertDataNascimento" style="display:none">
            <strong>Atenção!</strong> O campo Data de Nascimento é obrigatório!
        </div>
        <div class="alert alert-danger background-danger" role="alert" id="alertSalvar" style="display:none">
            <strong>Atenção!</strong> Erro ao salvar Voluntário, tente novamente!
        </div>

        <div id="accordion">

            <div class="card">
                <div class="accordion-heading" role="tab" id="headingOne">
                    <h3 class="card-title accordion-title">
                        <a class="accordion-msg" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            Dados Cadastrais
                        </a>
                    </h3>
                </div>
                <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                    <div class="card-body">
                        <div class="card-block">
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Dados Obrigatórios</legend>
                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label>Nome</label>
                                            <input asp-for="CadastroNome" class="form-control" type="text" id="txtNome" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="CadastroDataNascimento" class="form-control" onblur="calculaIdade()" type="date" id="dtDataNascimento" />
                                        </div>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="form-group">
                                            <label>Idade</label>
                                            <input asp-for="CadastroIdade" class="form-control" id="txtIdade" type="text" readonly />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>CPF</label>
                                            <input asp-for="CPF" class="form-control" type="text" maxlength="14" onkeypress="$(this).mask('000.000.000-00');" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>RG</label>
                                            <input asp-for="CadastroRG" class="form-control" id="txtRG" type="text" maxlength="15" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Classe Social</label>
                                            <select asp-for="CadastroClasseSocial" id="cboClasseSocial" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="A">A</option>
                                                <option value="B1">B1</option>
                                                <option value="B2">B2</option>
                                                <option value="C1">C1</option>
                                                <option value="C2">C2</option>
                                                <option value="D-E">D-E</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Telefones</legend>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Celular</label>
                                            <input asp-for="TelefoneCelular" name="TelefoneCelular" onkeypress="$(this).mask('(00) 0000-00009')" id="txtCelular" type="text" class="form-control telphone_with_code2" maxlength="15" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Whatsapp</label>
                                            <input asp-for="Whatsapp" name="Whatsapp" onkeypress="$(this).mask('(00) 0000-00009')" id="txtWhatsapp" type="text" class="form-control telphone_with_code2" maxlength="15" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Residencial</label>
                                            <input asp-for="TelefoneResidencial" name="TelefoneResidencial" onkeypress="$(this).mask('(00) 0000-0000')" type="text" class="form-control telphone_with_code" id="txtResidencial" maxlength="15" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Comercial</label>
                                            <input asp-for="TelefoneComercialRecado" name="TelefoneComercialRecado" onkeypress="$(this).mask('(00) 0000-0000')" type="text" class="form-control telphone_with_code" id="txtComercial" maxlength="15" />
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Informações Saúde</legend>
                                <div class="row">
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label>Capacidade de sentir cheiro é normal?</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="CapacidadeSentirCheiroNormal" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Sim
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="CapacidadeSentirCheiroNormal" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Não
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Diabético?</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Diabetico" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Sim
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Diabetico" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Não
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Fumante?</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Fumante" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Sim
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Fumante" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Não
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label>Alergia?</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Alergia" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Sim
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="Alergia" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Não
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-10">
                                        <div class="form-group">
                                            <label>Qual?</label>
                                            <input asp-for="AlergiaQual" class="form-control" type="text" maxlength="100" />
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Informações Complementares</legend>
                                <div class="row">
                                    <div class="col-md-7">
                                        <div class="form-group">
                                            <label>Logradouro</label>
                                            <input asp-for="Logradouro" class="form-control" type="text" maxlength="100" />
                                        </div>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="form-group">
                                            <label>Número</label>
                                            <input asp-for="Numero" class="form-control" type="number" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Bairro</label>
                                            <input asp-for="Bairro" class="form-control" type="text" maxlength="50" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-5">
                                        <div class="form-group">
                                            <label>Cidade</label>
                                            <input asp-for="Cidade" class="form-control" type="text" maxlength="50" />
                                        </div>
                                    </div>
                                    <div class="col-md-1">
                                        <div class="form-group">
                                            <label>UF</label>
                                            <input asp-for="UF" class="form-control" type="text" maxlength="2" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Complemento</label>
                                            <input asp-for="Complemento" class="form-control" type="text" maxlength="100" />
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label>CEP</label>
                                            <input asp-for="CEP" name="CEP" maxlength="10" class="form-control" type="text" onkeypress="$(this).mask('00.000-000')" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Profissão</label>
                                            <input asp-for="Profissao" class="form-control" type="text" maxlength="100" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Nome da Empresa</label>
                                            <input asp-for="NomeDaEmpresa" class="form-control" type="text" maxlength="100" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Ensino Fundamental</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoFundamentalCompleto" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Completo
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoFundamentalCompleto" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Incompleto
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Ensino Médio</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoMedioCompleto" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Completo
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoMedioCompleto" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Incompleto
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Ensino Superior</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoSuperiorCompleto" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Completo
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoSuperiorCompleto" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Incompleto
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Ensino Pós-Graduação</label>
                                            <div class="form-radio">
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoPosGraduacaoCompleta" value="1" data-bv-field="member">
                                                        <i class="helper"></i>Completo
                                                    </label>
                                                </div>
                                                <div class="radio radiofill radio-primary radio-inline">
                                                    <label>
                                                        <input type="radio" asp-for="EnsinoPosGraduacaoCompleta" value="0" data-bv-field="member">
                                                        <i class="helper"></i>Incompleto
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Observações</legend>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Primeira</label>
                                            <textarea asp-for="CadastroObsGerais" rows="5" cols="5" class="form-control" placeholder="Observações Gerais" id="txtObservacoesGerais"></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Segunda</label>
                                            <textarea asp-for="CadastroObs1" rows="5" cols="5" class="form-control" placeholder="Observações 1" id="txtObservacoes1"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Terceira</label>
                                            <textarea asp-for="CadastroObs2" rows="5" cols="5" class="form-control" placeholder="Observações 2" id="txtObservacoes2"></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Quarta</label>
                                            <textarea asp-for="CadastroObs3" rows="5" cols="5" class="form-control" placeholder="Observações 3" id="txtObservacoes3"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Filho</legend>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento1" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo1" id="FilhosSexo1" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento2" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo2" id="FilhosSexo2" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento3" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo3" id="FilhosSexo3" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento4" name="FilhosDataNascimento4" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo4" id="FilhosSexo4" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento5" name="FilhosDataNascimento5" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo5" id="FilhosSexo5" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Data de Nascimento</label>
                                            <input asp-for="FilhosDataNascimento6" name="FilhosDataNascimento6" class="form-control" type="date" id="dtDataNascimentoFilho" />
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Sexo</label>
                                            <select asp-for="FilhosSexo6" id="FilhosSexo6" class="form-control form-control-bold">
                                                <option value="0">-- selecione --</option>
                                                <option value="F">Feminino</option>
                                                <option value="M">Masculino</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr />
                        </div>
                    </div>
                </div>
            </div>

            <div class="card">
                <div class="accordion-heading" role="tab" id="headingTwo">
                    <h3 class="card-title accordion-title">
                        <a class="accordion-msg" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                            Produtos e Hábitos de Consumo
                        </a>
                    </h3>
                </div>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
                    <div class="card-body">
                        <div class="card-block">
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Sabonete Corporal</legend>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="checkbox-fade fade-in-primary; col-md-3">
                                            <label>
                                                <input type="checkbox" asp-for="SaboneteCorporalBarra" checked="@(1 == 0)">
                                                <span class="cr">
                                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                                </span>
                                                <span>Barra</span>
                                            </label>
                                        </div>
                                        <div class="checkbox-fade fade-in-primary; col-md-3">
                                            <label>
                                                <input type="checkbox" asp-for="SaboneteCorporalLiquido" checked="@(1 == 0)">
                                                <span class="cr">
                                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                                </span>
                                                <span>Líquido</span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <hr>
                            <fieldset class="border p-2">
                                <legend class="col-form-label pt-2">Sabonete Facial</legend>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="checkbox-fade fade-in-primary; col-md-3">
                                            <label>
                                                <input type="checkbox" asp-for="SaboneteFacialBarra" checked="@(1 == 0)">
                                                <span class="cr">
                                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                                </span>
                                                <span>Barra</span>
                                            </label>
                                        </div>
                                        <div class="checkbox-fade fade-in-primary; col-md-3">
                                            <label>
