@model GestaodeVoluntarios.UI.Models.TodosVoluntarios

<!-- order-card start -->
<div class="col-md-6 col-xl-3">
    <div class="card bg-c-blue order-card">
        <div class="card-block">
            <a href="~/VoluntariosMasculino">
                <h6 class="m-b-20">Masculinos Ativos</h6>
                <h2 class="text-right">
                    <i class="ti-tag f-left"></i><span>
                        <label asp-for="QtdeVoluntarioMasculinoAtivo" class="control-label">@Model.QtdeVoluntarioMasculinoAtivo</label>
                    </span>
                </h2>
            </a>
        </div>
    </div>
</div>
<div class="col-md-6 col-xl-3">
    <div class="card bg-c-pink order-card">
        <div class="card-block">
            <a href="~/VoluntariosMasculinoInativos">
                <h6 class="m-b-20">Masculinos Inativos</h6>
                <h2 class="text-right">
                    <i class="ti-tag f-left"></i><span>
                        <label asp-for="QtdeVoluntarioMasculinoInativo" class="control-label">@Model.QtdeVoluntarioMasculinoInativo</label>
                    </span>
                </h2>
            </a>
        </div>
    </div>
</div>
<div class="col-md-6 col-xl-3">
    <div class="card bg-c-blue order-card">
        <div class="card-block">
            <a href="~/VoluntariosFeminino">
                <h6 class="m-b-20">Femininos Ativos</h6>
                <h2 class="text-right">
                    <i class="ti-tag f-left"></i><span>
                        <label asp-for="QtdeVoluntarioFemininoAtivo" class="control-label">@Model.QtdeVoluntarioFemininoAtivo</label>
                    </span>
                </h2>
            </a>
        </div>
    </div>
</div>
<div class="col-md-6 col-xl-3">
    <div class="card bg-c-pink order-card">
        <a href="~/VoluntariosFemininoInativos">
            <div class="card-block">
                <h6 class="m-b-20">Femininos Inativos</h6>
                <h2 class="text-right"><i class="ti-tag f-left"></i><span> <label asp-for="QtdeVoluntarioFemininoInativo" class="control-label">@Model.QtdeVoluntarioFemininoInativo</label></span></h2>
            </div>
        </a>
    </div>
</div>
<!-- order-card end -->