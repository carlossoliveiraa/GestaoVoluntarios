    <script type="text/javascript">

    $(document).ready(function () {
        var perfil = $('#hdnPerfil').val();

        if (perfil == 'Consulta') {
            $('input').prop('disabled', true);
            $('select').prop('disabled', true);
            $('textarea').prop('disabled', true);
            $('#btnSalvar').css('visibility', 'hidden');
        }
    }
    ).on('click', '.voluntariosSalvar', function () {
        var btn = $(this),
            _id = btn[0].id;
        Validar();
    });


    function Validar() {

        var result = true;

        $('#alertNome').css('display', 'none');
        $('#alertClasseSocial').css('display', 'none');
        $('#alertCPF').css('display', 'none');
        $('#alertRG').css('display', 'none');
        $('#alertDataNascimento').css('display', 'none');

        var nome = $('#txtNome').val();
        if (nome == "") {
            $('#alertNome').css('display', 'block');
            result = false;
        }
        var classeSocial = $('#cboClasseSocial').val();
        if (classeSocial == "0") {
            $('#alertClasseSocial').css('display', 'block');
            result = false;
        }
        var CPF = $('#txtCPF').val();
        if (CPF == "") {
            $('#alertCPF').css('display', 'block');
            result = false;
        }
        var RG = $('#txtRG').val();
        if (RG == "") {
            $('#alertRG').css('display', 'block');
            result = false;
        }
        var DataNascimento = $('#dtDataNascimento').val();
        if (DataNascimento == "") {
            $('#alertDataNascimento').css('display', 'block');
            result = false;
        }

        if (result == false) {
            $('html,body').scrollTop(0);
        }

        if (result)
        {
            Salvar();
        }
    }

    function Salvar() {

        var id = $("#hdnId").val();
        var json = $("#formFemininoCadastro").serialize();

        $.ajax({
            url: "/VoluntariosFeminino/Edit/" + id,
            type: "POST",
            data: json,
            dataType: "html",
            success: function (data) {
                swal({
                    title: "Alterado!",
                    text: "Voluntário " + data + " alterado com sucesso!",
                    type: "success",
                },
                    function () {
                        window.location.href = '/VoluntariosFeminino/Index/';
                    }
                );
            },
            error: function (data) {
                swal("Oops", "Não foi possível conectar ao servidor!", "error");
            }
        });
    }

    function calculaIdade() {
        var date = $('#dtDataNascimento').val();
        var dataaniversario = date.split('-');
        var d = new Date,
            ano_atual = d.getFullYear(),
            mes_atual = d.getMonth() + 1,
            dia_atual = d.getDate(),

            ano_aniversario = +dataaniversario[0],
            mes_aniversario = +dataaniversario[1],
            dia_aniversario = +dataaniversario[2],

            quantos_anos = ano_atual - ano_aniversario;

        if (mes_atual < mes_aniversario || mes_atual == mes_aniversario && dia_atual < dia_aniversario) {
            quantos_anos--;
        }

        $("#txtIdade").val(quantos_anos);
    }

    </script>
