@model GestaodeVoluntarios.UI.Models.Feminino.FemininoCadastro
    <style>

        legend {
            background-color: #EFEFEF;
            color: black;
            padding: 5px 20px;
        }

        input {
            margin: 5px;
        }
    </style>

    <div class="col-sm-12">
        <input type="hidden" value="@ViewBag.PerfilUsuario" id="hdnPerfil" />
        <input type="hidden" value="@ViewBag.Id" id="hdnId" />
        
        <form asp-action="Edit" method="post" id="formFemininoCadastro">
            <div class="page-header card">
                <div class="card-block">
                    <h5 class="m-b-10">Voluntário Feminino</h5>
                    <ul class="breadcrumb-title b-t-default p-t-10">
                        <li class="breadcrumb-item">
                            <a href="\Home\index"> <i class="fa fa-home"></i> </a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="\VoluntariosFeminino\index">Voluntário Feminino</a>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="alert alert-warning background-danger" role="alert" id="alertNome" style="display:none">
                <strong>Atenção!</strong> O campo Nome é obrigatório!
            </div>
            <div class="alert alert-warning background-danger" role="alert" id="alertClasseSocial" style="display:none">
                <strong>Atenção!</strong> O campo Classe Social é obrigatório!
            </div>
            <div class="alert alert-warning background-danger" role="alert" id="alertCPF" style="display:none">
                <strong>Atenção!</strong> O campo CPF é obrigatório!
            </div>
            <div class="alert alert-warning background-danger" role="alert" id="alertRG" style="display:none">
                <strong>Atenção!</strong> O campo RG é obrigatório!
            </div>
            <div class="alert alert-warning background-danger" role="alert" id="alertDataNascimento" style="display:none">
                <strong>Atenção!</strong> O campo Data de Nascimento é obrigatório!
            </div>
            <div class="alert alert-danger background-danger" role="alert" id="alertSalvar" style="display:none">
                <strong>Atenção!</strong> Erro ao salvar Voluntário, tente novamente!
            </div>

            <div id="accordion">

                <div class="card">
                    <div class="accordion-heading" role="tab" id="headingOne">
                        <h3 class="card-title accordion-title">
                            <a class="accordion-msg" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                Dados Cadastrais
                            </a>
                        </h3>
                    </div>
                    <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                        <div class="card-body">
                            <div class="card-block">
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Dados Obrigatórios</legend>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label>Nome</label>
                                                <input style="display:none" asp-for="FemininoCadastroId" class="form-control" value="@Model.FemininoCadastroId" type="text" id="txtFemininoCadastroId" />
                                                <input asp-for="CadastroNome" name="CadastroNome" maxlength="100" class="form-control" type="text" id="txtNome" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Data de Nascimento</label>
                                                <input asp-for="CadastroDataNascimento" name="CadastroDataNascimento" id="dtDataNascimento" class="form-control" onblur="calculaIdade()" type="date" maxlength="10" />
                                            </div>
                                        </div>
                                        <div class="col-md-1">
                                            <div class="form-group">
                                                <label>Idade</label>
                                                <input asp-for="CadastroIdade" name="CadastroIdade" class="form-control" id="txtIdade" type="text" readonly maxlength="3" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>CPF</label>
                                                <input asp-for="DadosPessoaisCPF" name="DadosPessoaisCPF" class="form-control" type="text" maxlength="14" onkeypress="$(this).mask('000.000.000-00');" />
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>RG</label>
                                                <input asp-for="CadastroRG" name="CadastroRG" class="form-control" id="txtRG" type="text" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Classe Social</label>
                                                <select asp-for="CadastroClasseSocial" name="CadastroClasseSocial" id="CadastroClasseSocial" class="form-control form-control-bold">
                                                    <option value="0">-- selecione --</option>
                                                    <option value="A">A</option>
                                                    <option value="B1">B1</option>
                                                    <option value="B2">B2</option>
                                                    <option value="C1">C1</option>
                                                    <option value="C2">C2</option>
                                                    <option value="D-E">D-E</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <br>
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Telefones</legend>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Celular</label>
                                                <input asp-for="CadastroTelefoneCelular" name="CadastroTelefoneCelular" onkeypress="$(this).mask('(00) 0000-00009')" id="CadastroTelefoneCelular" type="text" class="form-control telphone_with_code2" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Whatsapp</label>
                                                <input asp-for="CadastroTelefoneWhatsapp" name="CadastroTelefoneWhatsapp" onkeypress="$(this).mask('(00) 0000-00009')" id="CadastroTelefoneWhatsapp" type="text" class="form-control telphone_with_code2" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Residencial</label>
                                                <input asp-for="CadastroTelefoneResidencial" name="CadastroTelefoneResidencial" onkeypress="$(this).mask('(00) 0000-0000')" type="text" class="form-control telphone_with_code" id="CadastroTelefoneResidencial" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Comercial</label>
                                                <input asp-for="CadastroTelefoneComercialRecado" name="CadastroTelefoneComercialRecado" onkeypress="$(this).mask('(00) 0000-0000')" type="text" class="form-control telphone_with_code" id="CadastroTelefoneComercialRecado" maxlength="15" />
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <br>
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Informações Saúde</legend>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Olfato normal</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroCapacidadeSentirCheiro" asp-for="CadastroCapacidadeSentirCheiro" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroCapacidadeSentirCheiro" asp-for="CadastroCapacidadeSentirCheiro" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Diabético</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroDiabetico" asp-for="CadastroDiabetico" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroDiabetico" asp-for="CadastroDiabetico" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Fumante</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroFumante" asp-for="CadastroFumante" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroFumante" asp-for="CadastroFumante" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Grávida</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="Gravida" asp-for="Gravida" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="Gravida" asp-for="Gravida" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Amamenta</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="Amamenta" asp-for="Amamenta" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="Amamenta" asp-for="Amamenta" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Alergia</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroAlergiaSimNao" asp-for="CadastroAlergiaSimNao" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Sim
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="CadastroAlergiaSimNao" asp-for="CadastroAlergiaSimNao" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Não
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-7">
                                            <div class="form-group">
                                                <label>Qual alergia</label>
                                                <input asp-for="CadastroAlergiaQual" name="CadastroAlergiaQual" maxlength="100" class="form-control" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <br>
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Informações Complementares</legend>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Logradouro</label>
                                                <input asp-for="DadosPessoaisLogradouro" name="DadosPessoaisLogradouro" maxlength="100" class="form-control" type="text" />
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Número</label>
                                                <input asp-for="DadosPessoaisNumero" name="DadosPessoaisNumero" maxlength="5" class="form-control" type="number" />
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Bairro</label>
                                                <input asp-for="DadosPessoaisBairro" name="DadosPessoaisBairro" maxlength="50" class="form-control" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label>Cidade</label>
                                                <input asp-for="DadosPessoaisCidade" name="DadosPessoaisCidade" maxlength="50" class="form-control" type="text" />
                                            </div>
                                        </div>
                                        <div class="col-md-1">
                                            <div class="form-group">
                                                <label>UF</label>
                                                <input asp-for="DadosPessoaisUF" name="DadosPessoaisUF" maxlength="2" class="form-control" type="text" />
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Complemento</label>
                                                <input asp-for="DadosPessoaisComplementoBlocoApto" name="DadosPessoaisComplementoBlocoApto" maxlength="100" class="form-control" type="text" />
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>CEP</label>
                                                <input asp-for="DadosPessoaisCEP" name="DadosPessoaisCEP" maxlength="10" class="form-control" type="text" onkeypress="$(this).mask('00.000-000')" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Profissão</label>
                                                <input asp-for="ProfissaoProfissao" name="ProfissaoProfissao" maxlength="100" class="form-control" type="text" />
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Nome da Empresa</label>
                                                <input asp-for="ProfissaoNomeEmpresa" name="ProfissaoNomeEmpresa" maxlength="100" class="form-control" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Ensino Fundamental</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoFundamentalCompleto" asp-for="EnsinoFundamentalCompleto" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Completo
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoFundamentalCompleto" asp-for="EnsinoFundamentalCompleto" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Incompleto
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Ensino Médio</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoMedioCompleto" asp-for="EnsinoMedioCompleto" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Completo
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoMedioCompleto" asp-for="EnsinoMedioCompleto" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Incompleto
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Ensino Superior</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoSuperiorCompleto" asp-for="EnsinoSuperiorCompleto" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Completo
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoSuperiorCompleto" asp-for="EnsinoSuperiorCompleto" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Incompleto
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Ensino Pós-Graduação</label>
                                                <div class="form-radio">
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoPosGraduacaoCompleta" asp-for="EnsinoPosGraduacaoCompleta" value="1" data-bv-field="member">
                                                            <i class="helper"></i>Completo
                                                        </label>
                                                    </div>
                                                    <div class="radio radiofill radio-primary radio-inline">
                                                        <label>
                                                            <input type="radio" name="EnsinoPosGraduacaoCompleta" asp-for="EnsinoPosGraduacaoCompleta" value="0" data-bv-field="member">
                                                            <i class="helper"></i>Incompleto
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <br>
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Observações</legend>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Primeira</label>
                                                <textarea asp-for="CadastroObsGerais" name="CadastroObsGerais" maxlength="200" rows="5" cols="5" class="form-control" placeholder="Observações Gerais" id="txtObservacoesGerais"></textarea>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Segunda</label>
                                                <textarea asp-for="CadastroObs1" name="CadastroObs1" maxlength="200" rows="5" cols="5" class="form-control" placeholder="Observações 1" id="txtObservacoes1"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Terceira</label>
                                                <textarea asp-for="CadastroObs2" name="CadastroObs2" maxlength="200" rows="5" cols="5" class="form-control" placeholder="Observações 2" id="txtObservacoes2"></textarea>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Quarta</label>
                                                <textarea asp-for="CadastroObs3" name="CadastroObs3" maxlength="200" rows="5" cols="5" class="form-control" placeholder="Observações 3" id="txtObservacoes3"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <br>
                                <fieldset class="border p-2">
                                    <legend class="col-form-label pt-2">Filho</legend>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Data de Nascimento</label>
                                                <input asp-for="FilhoDataNascimento1" name="FilhoDataNascimento1" maxlength="10" class="form-control" type="date" id="FilhosDataNascimento1" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Sexo</label>
                                                <select asp-for="FilhoSexo1" name="FilhoSexo1" id="FilhosSexo1" class="form-control form-control-bold">
                                                    <option value="0">-- selecione --</option>
                                                    <option value="F">Feminino</option>
                                                    <option value="M">Masculino</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Data de Nascimento</label>
                                                <input asp-for="FilhoDataNascimento2" name="FilhoDataNascimento2" maxlength="10" class="form-control" type="date" id="FilhosDataNascimento2" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Sexo</label>
                                                <select asp-for="FilhoSexo2" name="FilhoSexo2" id="FilhosSexo2" class="form-control form-control-bold">
                                                    <option value="0">-- selecione --</option>
                                                    <option value="F">Feminino</option>
                                                    <option value="M">Masculino</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Data de Nascimento</label>
                                                <input asp-for="FilhoDataNascimento3" name="FilhoDataNascimento3" maxlength="10" class="form-control" type="date" id="FilhosDataNascimento3" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Sexo</label>
                                                <select asp-for="FilhoSexo3" name="FilhoSexo3" id="FilhosSexo3" class="form-control form-control-bold">
                                                    <option value="0">-- selecione --</option>
                                                    <option value="F">Feminino</option>
                                                    <option value="M">Masculino</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Data de Nascimento</label>
                                                <input asp-for="FilhoDataNascimento4" name="FilhoDataNascimento4" maxlength="10" class="form-control" type="date" id="FilhosDataNascimento4" />
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Sexo</label>
                                                <select asp-for="FilhoSexo4" name="FilhoSexo4" id="FilhosSexo4" class="form-control form-control-bold">
                                                    <option value="0">-- selecione --</option>
                                                    <option value="F">Feminino</option>
                                                    <option value="M">Masculino</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
